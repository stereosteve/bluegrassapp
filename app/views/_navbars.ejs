<%- startview('navbar') %>

  <div id="topbar">
    <div class="right" ng-show="showArrows">
      <a ng-click="prevPage()"><i class="icon-chevron-left"></i></a>
      <a ng-click="nextPage()"><i class="icon-chevron-right"></i></a>
    </div>
    <a onclick="history.go(-1)"><i class="icon-arrow-left"></i></a>
    <a ng-click="toggleMenu()"><i class="icon-reorder"></i></a>
  </div>

  <div style="margin-top: 50px;"></div>

  <div id="leftmenu" ng-show="showMenu">
    <div class="banner">
      <a href="#/home">Home</a>
      <a href="#/songs">Songs</a>
      <a href="#/favs">Favorites</a>
      <a href="#/about">About</a>
    </div>
  </div>

<%- endview %>
