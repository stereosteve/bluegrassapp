!= startPartial('_home')
  h1 Home
!= endPartial()

!= startPartial('_songs')
  h1 {{letter}} Songs

  span(ng-repeat="l in alphabet")
    | <a ng-click="setLetter(l)">{{l}}</a> | 

  li(ng-repeat="song in songs | filter:letterFilter")
    a(href="/songs/{{song.id}}") {{song.name}}

!= endPartial()

!= startPartial('_song')
  a(href="/songs") Songs
  h1 {{song.name}}
  h2 {{song.artist}}
  pre {{song.lyrics}}
!= endPartial()
